def przesun_czarne_do_gory(tablica):
    wysokosc = len(tablica)
    szerokosc = len(tablica[0])

    for kolumna in range(szerokosc):
        kolumna_tablicy = [tablica[wiersz][kolumna] for wiersz in range(wysokosc)]
        kolumna_bez_czarnych = [blok for blok in kolumna_tablicy if blok != "â¬›"]
        kolumna_tablicy = ["â¬›"] * (wysokosc - len(kolumna_bez_czarnych)) + kolumna_bez_czarnych

        for wiersz in range(wysokosc):
            tablica[wiersz][kolumna] = kolumna_tablicy[wiersz]

    return tablica


def group_positions(positions):
    groups = []

    for pos in positions:
        added_to_group = False
        for group in groups:
            for group_pos in group:
                if abs(pos[0] - group_pos[0]) <= 1 and abs(pos[1] - group_pos[1]) <= 1:
                    group.append(pos)
                    added_to_group = True
                    break
            if added_to_group:
                break

        if not added_to_group:
            groups.append([pos])
    if printing:print(f' groups: {groups}')
    return groups


def explode_gem(board, y, x, strin,move, board_to_overwrite, original_board):
    if printing:
        print(f'board state:')
        score = 0

        for row in board:
            print(' '.join(row))


    board2 = copy.deepcopy(board)

    board2[y][x] == 'ğŸŸ£'
    if printing:
        print(f'{strin}, x:{x}, y:{y} ')

        print('board to voerwrite')
        for row in board_to_overwrite:
            print(' '.join(row))

    if printing:print('---------------------')
    pos_of_powered = []
    powered_gems_array = ['ğŸŸ£', 'âšª', 'ğŸŸ ', 'ğŸ”´', 'ğŸŸ¢', 'ğŸ”µ', 'ğŸŸ¡']

    for i in range(8):
        for j in range(8):
            if board2[j][i] in powered_gems_array:
                pos = (i, j)
                pos_of_powered.append(pos)

    if printing:print(f'powered gem pos {pos_of_powered}')

    gem_groups = group_positions(pos_of_powered)
    gem = (x, y)
    group_of_gems = []  # chain
    for group in gem_groups:
        if printing:print(gem, group, 'zwx')
        if gem in group:
            if printing:print(f'gem found in group {group}')
            group_of_gems = group
    # print(f' remaining pos {pos}')
    if printing:print(f'group_of_gems: {group_of_gems}')
    for pos in group_of_gems:
        if printing:print(pos, 'abc')
        for i in range(pos[0] - 1, pos[0] + 2):
            for j in range(pos[1] - 1, pos[1] + 2):
                if i > -1 and i < 8 and j > -1 and j < 8:
                    board2[j][i] = 'â¬›'
    if strin == 'bomb!':
        gem = (x, y)
    if gem[1] < 10:                     board2[gem[1]][gem[0]] = 'â¬›'
    if gem[1] + 1 < 8:                  board2[gem[1] + 1][gem[0]] = 'â¬›'
    if gem[1] + 1 < 8 and gem[0] + 1 < 8: board2[gem[1] + 1][gem[0] + 1] = 'â¬›'
    if gem[1] + 1 < 8 and gem[0] - 1 > -1: board2[gem[1] + 1][gem[0] - 1] = 'â¬›'
    if gem[0] + 1 < 8:                  board2[gem[1]][gem[0] + 1] = 'â¬›'
    if gem[0] - 1 > -1:                  board2[gem[1]][gem[0] - 1] = 'â¬›'
    if gem[1] - 1 > -1 and gem[0] + 1 < 8: board2[gem[1] - 1][gem[0] + 1] = 'â¬›'
    if gem[1] - 1 > -1:                  board2[gem[1] - 1][gem[0]] = 'â¬›'
    if gem[1] - 1 > -1 and gem[0] - 1 > -1: board2[gem[1] - 1][gem[0] - 1] = 'â¬›'
    if printing:
        print('state of board after explode')
        for row in board2:
            print(' '.join(row))

        print('state of board after explode with other black places')

    '''for i in range(8):
        for j in range(8):
            if board2[j][i] == 'â¬›' or board2[j][i] == 'ğŸŒˆ':
                board2[j][i] = "â¬›"'''
    if printing:
        for row in board2:
            print(' '.join(row))
    #board2 = original_board
    if printing:print('------------------------')
    wynik = przesun_czarne_do_gory(board2)
    if printing:
        for wiersz in wynik:
            print(" ".join(wiersz))
        print('wynik')
    print('gfd')
    c,d = check_for_rows(board2,board_to_overwrite,move,original_board,[])
    for row in c:
        print(' '.join(row))
    print('s[erma')
    return c


def convert_board(board):
    if printing:
        print(f'{bcolors.OKBLUE}=== convert board function:')
        print(board)
        for row in board:
            print(' '.join(row))
        print('^^^^ convert board')
    converted_board = board
    emoji_mapping = {
        'ğŸŸ§': 'ğŸŸ§',
        'ğŸŸ¨': 'ğŸŸ¨',
        'ğŸŸ¦': 'ğŸŸ¦',
        'ğŸŸ©': 'ğŸŸ©',
        'ğŸŸª': 'ğŸŸª',
        'â¬œ': 'â¬œ',
        'ğŸŸ¥': 'ğŸŸ¥',
        'ğŸŒˆ': 'ğŸŒˆ',
        'âšª': 'â¬œ',
        'ğŸ”µ': 'ğŸŸ¦',
        'ğŸ”´': 'ğŸŸ¥',
        'ğŸŸ£': 'ğŸŸª',
        'ğŸŸ¢': 'ğŸŸ©',
        'ğŸŸ ': 'ğŸŸ§',
        'ğŸŸ¡': 'ğŸŸ¨',
        'â¬›': 'â¬›',
        'ğŸŒ«ï¸': 'ğŸŒ«ï¸',
        'ğŸ…¿ï¸': 'âš«'
    }
    for i in range(8):
        for j in range(8):
            gem = board[i][j]
            if gem in emoji_mapping:
                converted_board[i][j] = emoji_mapping[gem]
            else:
                converted_board[i][j] = gem
    if printing:
        print(f'copnvert board return functionb {bcolors.ENDC}')
        for row in converted_board:
            print(' '.join(row))
    return converted_board





def next_move(board_p, moves, already_done_moves, orig_board):
    if printing:print(f'{bcolors.WARNING}start of next move  fu nction')

