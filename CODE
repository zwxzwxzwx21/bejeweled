                    if (final_board[i][j] == final_board[i - 1][j + 1] and
                            final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                            final_board[i][j] != 'ğŸŒ«ï¸'):
                        if (final_board[i][j] == final_board[i - 2][j] and
                                final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                final_board[i][j] != 'ğŸŒ«ï¸'):
                            if (final_board[i][j] == final_board[i - 3][j] and
                                    final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                    final_board[i][j] != 'ğŸŒ«ï¸'):
                                # print('â””d')
                                move = (j, i, 'up', 'â”´d')
                                if move not in moves_to_up:
                                    moves_to_up.append(move)
    # print('$$$$$$$$$$$$$$ MOVES BY 5 $$$$$$$$$$$$$$$$$')
    for i in range(8):
        for j in range(8):

            if j + 2 < 8:
                if (final_board[i][j] == final_board[i][j + 2] and
                        final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                        final_board[i][j] != 'ğŸŒ«ï¸' and
                        final_board[i][j + 2] != 'â¬›' != 'ğŸŒˆ' and
                        final_board[i][j + 2] != 'ğŸŒ«ï¸'):  # sprawdzianie czy gem o 2 jest taki sam
                    if i + 1 < 8 and j + 1 < 8:
                        if (final_board[i][j] == final_board[i + 1][j + 1] and
                                final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                final_board[i][j] != 'ğŸŒ«ï¸' and
                                final_board[i + 1][j + 1] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                final_board[i + 1][j + 1] != 'ğŸŒ«ï¸'):
                            if j + 3 < 8 and j - 1 > -1:
                                if (final_board[i][j] == final_board[i][j + 3] and
                                        final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i][j + 3] != 'â¬›' and final_board[i][j + 3] != 'ğŸŒˆ' and
                                        final_board[i][j + 3] != 'ğŸŒ«ï¸' and
                                        final_board[i][j] == final_board[i][j - 1] and
                                        final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i][j - 1] != 'â¬›' and final_board[i][j - 1] != 'ğŸŒˆ' and
                                        final_board[i][j - 1] != 'ğŸŒ«ï¸'):
                                    # print(f' five move found with gem at x: {j+1} y: {i+1}, second gem at {j-1} {i}  third gem at {j} {i} fourth {j+2} {i} fifth gem at x: {j+3} {i}')
                                    move = (j + 1, i + 1, 'up', 5)
                                    if move not in moves_to_up:
                                        moves_to_up.append(move)
                    if i - 1 > -1 and j + 1 < 8:
                        if (final_board[i][j] == final_board[i - 1][j + 1] and
                                final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                final_board[i][j] != 'ğŸŒ«ï¸' and
                                final_board[i - 1][j + 1] != 'â¬›' and final_board[i - 1][j + 1] != 'ğŸŒˆ' and
                                final_board[i - 1][j + 1] != 'ğŸŒ«ï¸'):
                            if j + 3 < 8 and j - 1 > -1:
                                if (final_board[i][j] == final_board[i][j + 3] and
                                        final_board[i][j] != 'â¬›' and final_board[i][j] != 'ğŸŒˆ' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i][j + 3] != 'â¬›' and final_board[i][j + 3] != 'ğŸŒˆ' and
                                        final_board[i][j + 3] != 'ğŸŒ«ï¸' and
                                        final_board[i][j] == final_board[i][j - 1] and
                                        final_board[i][j] != 'â¬›' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i][j] != 'ğŸŒˆ' and
                                        final_board[i][j - 1] != 'â¬›' != 'ğŸŒˆ' and
                                        final_board[i][j - 1] != 'ğŸŒ«ï¸' and
                                        final_board[i][j - 1] != 'ğŸŒˆ'):
                                    # print(f' five move found with gem at x: {j+1} y: {i-1}, second gem at {j-1} {i} third gem at {j} {i} fourth {j + 2} {i} fifth gem at x: {j + 3} {i}')
                                    move = (j + 1, i - 1, 'down', 5)
                                    if move not in moves_to_down:
                                        moves_to_down.append(move)

    # print("@@@@@@@@@@@@@@@ 5 moves vertical; @@@@@@@@@@@@@@@@@@")
    for i in range(8):
        for j in range(8):

            if i + 2 < 8:
                if (final_board[i][j] == final_board[i + 2][j] and
                        final_board[i][j] != 'â¬›' != 'ğŸŒˆ' and
                        final_board[i][j] != 'ğŸŒ«ï¸' and
                        final_board[i][j] != 'ğŸŒˆ' and
                        final_board[i + 2][j] != 'â¬›' != 'ğŸŒˆ' and
                        final_board[i + 2][j] != 'ğŸŒ«ï¸' and
                        final_board[i + 2][j] != 'ğŸŒˆ'):
                    if i + 1 < 8 and j + 1 < 8:
                        if (final_board[i][j] == final_board[i + 1][j + 1] and
                                final_board[i][j] != 'â¬›' != 'ğŸŒˆ' and
                                final_board[i][j] != 'ğŸŒ«ï¸' and
                                final_board[i][j] != 'ğŸŒˆ' and
                                final_board[i + 1][j + 1] != 'â¬›' != 'ğŸŒˆ' and
                                final_board[i + 1][j + 1] != 'ğŸŒ«ï¸' and
                                final_board[i + 1][j + 1] != 'ğŸŒˆ'):
                            if i + 3 < 8 and i - 1 > -1:
                                if (final_board[i][j] == final_board[i + 3][j] and
                                        final_board[i][j] != 'â¬›' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i][j] != 'ğŸŒˆ' and
                                        final_board[i + 3][j] != 'â¬›' != 'ğŸŒˆ' and
                                        final_board[i + 3][j] != 'ğŸŒ«ï¸' and
                                        final_board[i + 3][j] != 'ğŸŒˆ' and
                                        final_board[i][j] == final_board[i - 1][j] and
                                        final_board[i][j] != 'â¬›' != 'ğŸŒˆ' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i - 1][j] != 'â¬›' and
                                        final_board[i - 1][j] != 'ğŸŒ«ï¸' and
                                        final_board[i - 1][j] != 'ğŸŒˆ'):
                                    # print(f' five move found with gem at x: {j+1} y: {i+1}, second gem at {j} {i - 1} third {j} {i} fourth gem at x: {j} {i + 2} fifth gem at x: {j} {i+3}')
                                    move = (j + 1, i + 1, 'left', 5)
                                    if move not in moves_to_left:
                                        moves_to_left.append(move)
                    if i + 1 < 8 and j - 1 > -1:
                        if (final_board[i][j] == final_board[i + 1][j - 1] and
                                final_board[i][j] != 'â¬›' and
                                final_board[i][j] != 'ğŸŒˆ' and
                                final_board[i][j] != 'ğŸŒ«ï¸' and
                                final_board[i + 1][j - 1] != 'â¬›' and
                                final_board[i + 1][j - 1] != 'ğŸŒ«ï¸' and
                                final_board[i + 1][j - 1] != 'ğŸŒˆ'):
                            if i + 3 < 8 and i - 1 > -1:
                                if (final_board[i][j] == final_board[i + 3][j] and
                                        final_board[i][j] != 'â¬›' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i][j] != 'ğŸŒˆ' and
                                        final_board[i + 3][j] != 'â¬›' and
                                        final_board[i + 3][j] != 'ğŸŒ«ï¸' and
                                        final_board[i + 3][j] != 'ğŸŒˆ' and
                                        final_board[i][j] == final_board[i - 1][j] and
                                        final_board[i][j] != 'â¬›' != 'ğŸŒˆ' and
                                        final_board[i][j] != 'ğŸŒ«ï¸' and
                                        final_board[i - 1][j] != 'â¬›' and
                                        final_board[i - 1][j] != 'ğŸŒ«ï¸' and final_board[i - 1][j] != 'ğŸŒˆ'):
                                    # print(f' five move found with gem at x: {j-1} y: {i+1}, second gem at {j} {i - 1} third {j} {i} fourth gem at x: {j} {i + 2} fifth gem at x {j} {i+3}')
                                    move = (j - 1, i + 1, 'right', 5)
                                    if move not in moves_to_right:
                                        moves_to_right.append(move)


    if printing:
        print(f'{bcolors.BOLD} MOVES {bcolors.ENDC}')
        if len(moves_to_left) != 0:
            print(f'{bcolors.BOLD}{bcolors.OKCYAN} TO LEFT: {moves_to_left}')
        if len(moves_to_right) != 0:
            print(f'{bcolors.BOLD}{bcolors.OKCYAN} TO RIGHT: {moves_to_right}')
        if len(moves_to_up) != 0:
            print(f'{bcolors.BOLD}{bcolors.OKCYAN} TO UP: {moves_to_up}')
        if len(moves_to_down) != 0:
            print(f'{bcolors.BOLD}{bcolors.OKCYAN} TO DOWN: {moves_to_down}{bcolors.ENDC}')

    all_moves = []
    for move in moves_to_down:
        all_moves.append(move)
    for move in moves_to_left:
        all_moves.append(move)
    for move in moves_to_right:
        all_moves.append(move)
    for move in moves_to_up:
        all_moves.append(move)

    # print(f'all the moves that will be checked: {all_moves} with lenght of {len(all_moves)}')

    # original = with powered | final = without powered
    return all_moves

