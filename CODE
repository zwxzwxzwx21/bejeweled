                    final_board[(vertical_pair_of_gems[i][1][1])][(vertical_pair_of_gems[i][1][0])] != 'ğŸŒˆ' and
                    final_board[(vertical_pair_of_gems[i][1][1]) + 2][(vertical_pair_of_gems[i][0][0])] != 'â¬›' and
                    final_board[(vertical_pair_of_gems[i][1][1]) + 2][(vertical_pair_of_gems[i][0][0])] != 'ğŸŒ«ï¸' and
                    final_board[(vertical_pair_of_gems[i][1][1]) + 2][(vertical_pair_of_gems[i][0][0])] != 'ğŸŒˆ'):

