                   ['â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›'],
                   ['â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›'],
                   ['â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›']]
    copy_of_copy_of_p = copy.deepcopy(copy_of_p)
    board_not_powered = convert_board(copy_of_copy_of_p)
    if printing:
        print('board powered')
        for row in copy_of_p:
            print(' '.join(row))
        print('board not powered')
        for row in board_not_powered:
            print(' '.join(row))
        print('rty', moves)
    sieved_m = sieve_moves(moves)
    bomb_placement = []
