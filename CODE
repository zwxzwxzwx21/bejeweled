        if printing:
            for row in copy_p:
                print(' '.join(row))
            print('---dfg---')
            for row in copy_np:
                print(' '.join(row))
        '''a = copy.deepcopy(copy_p)
        copy_np = convert_board(a)'''
        powered = ['âšª', 'ğŸ”µ', 'ğŸ”´', 'ğŸŸ£', 'ğŸŸ¢', 'ğŸŸ ', 'ğŸŸ¡']
        if move[2] == 'left':
            if move[3] != 'R':
                if printing:print('Ä†', copy_np[move[1]][move[0]], copy_np[move[1]][move[0]])
                try:
                    for i in range(8):
                        for j in range(8):
