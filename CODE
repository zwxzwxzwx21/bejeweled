    copy_of_p = copy.deepcopy(board_p)
    black_board = [['â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›'],
                   ['â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›', 'â¬›'],
