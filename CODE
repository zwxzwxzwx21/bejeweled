                                2] == 'left':
                                # print('4')
                                to_remove.append(moves2)
                # print('moves n shit',moves,moves2)
    for move in array:
        if move not in to_remove:
            sieved.append(move)
    # print(to_remove)

    if printing:print(f'sieved returning {sieved}')
    return sieved


