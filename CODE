                                copy_p[j][i] = copy_p[j][i]-1

                except:
                    pass
                copy_np[move[1]][move[0]], copy_np[move[1] - 1][move[0]] = copy_np[move[1] - 1][
                    move[0]], copy_np[move[1]][move[0]]
                copy_p[move[1]][move[0]], copy_p[move[1] - 1][move[0]] = copy_p[move[1] - 1][
                    move[0]], copy_p[move[1]][move[0]]
            elif move[3] == 'R':
