def sieve_moves(array):
    sieved = []
    to_remove = []
    for moves in array:
        for moves2 in array:
            moves_4 = ['4l', '4u', '4d', '4r']
            if moves != moves2:
                if moves[0] == moves2[0] and moves[1] == moves2[1] and moves[2] == moves2[2]:
                    if printing:
                        print(moves, moves2)

                    if moves[3] == 'â”˜r' and moves2[3] == 3:
                        to_remove.append(moves2)
                        if printing:
                            print(f'remove {moves2} cuz {moves}1265')
                    if moves[3] == 'â”˜d' and moves2[3] == 3:
                        to_remove.append(moves2)
                        if printing:
                            print(f'remove {moves2} cuz {moves}1268')
                    if moves[3] == 'â””l' and moves2[3] == 3:
                        to_remove.append(moves2)
                        if printing:
                            print(f'remove {moves2} cuz {moves}1271')
                    if moves[3] == 'â”u' and moves2[3] == 3:
                        to_remove.append(moves2)
                        if printing:
                            print(f'remove {moves2} cuz {moves}1274')
                    if moves[3] == 'â”Œu' and moves2[3] == 3:
                        to_remove.append(moves2)
                        if printing:
                            print(f'remove {moves2} cuz {moves}1277')
                    if moves[3] == 'â”r' and moves2[3] == 3:
                        to_remove.append(moves2)
                        if printing:
